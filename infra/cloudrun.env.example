# App
OTEL_SERVICE_NAME=gemini-cost-guardian
OTEL_RESOURCE_ATTRIBUTES=service.version=0.1.0,deployment.environment=prod

# Datadog (US5)
OTEL_EXPORTER_OTLP_TRACES_ENDPOINT=http://localhost:4318/v1/traces
OTEL_EXPORTER_OTLP_METRICS_ENDPOINT=http://localhost:4318/v1/metrics
OTEL_EXPORTER_OTLP_METRICS_TEMPORALITY_PREFERENCE=Delta
OTEL_TRACES_SAMPLER=always_on

# Secrets (DO NOT COMMIT REAL VALUES)
DD_API_KEY=__SET_IN_CLOUD_RUN__
DD_APP_KEY=__SET_IN_CLOUD_RUN__

# Vertex AI / Gemini
GOOGLE_CLOUD_PROJECT=gemini-cost-guardian-2025
GOOGLE_CLOUD_LOCATION=global
GOOGLE_GENAI_USE_VERTEXAI=true
GCG_GEMINI_MODEL=gemini-2.5-flash